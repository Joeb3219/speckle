fn main(){
	
	var x = 0;
	var result = 0;

	while(x <= 12){
		result = x * x;
		
		printn(x);
		printc(' ');
		printc('*');
		printc(' ');
		printn(x);
		printc(' ');
		printc('=');
		printc(' ');

		printn(result);
		newline();
	
		x = x + 1;

	};

	printc('D'); printc('o'); printc('n'); printc('e');
	newline();

	ret 0;

}
