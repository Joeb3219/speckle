fn prints(str){
	var size = len(str);
	size = size - 1;

	var i = 0;

	while(i <= size){
		printc(str{i});
		i = i + 1;
	};

	newline();
}

fn main(){
	
	var arr = {5};

	arr{1} = 'e';
	arr{2} = 'l';
	arr{0} = 'H';
	arr{3} = 'l';
	arr{4} = 'o';

	prints(arr);

	var arr2 = {2};
	arr2{0} = {8};
	arr2{1} = {4};

	printn(len(arr2)); newline();
	printn(len(arr2{0})); newline();
	printn(len(arr2{1})); newline();

	ret 0;
}